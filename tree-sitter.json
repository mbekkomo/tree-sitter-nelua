{
  "grammars": [
    {
      "name": "nelua",
      "camelcase": "Nelua",
      "scope": "source.nelua",
      "path": ".",
      "file-types": [
        "nelua"
      ],
      "first-line-regex": "#!.*\\bnelua$",
      "injection-regex": "^nelua$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A Nelua grammar for tree-sitter",
    "authors": [
      {
        "name": "Komo",
        "email": "afiqquraisyzulkarnain@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/mbekkomo/tree-sitter-nelua"
    },
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
